{
    "files.associations": {
        "*.examples": "makefile",
        "*.modules": "makefile",
        "*.scons": "python",
        "stdlib.h": "c",
        "stdio.h": "c",
        "signal.h": "c",
        "ais_comm.h": "c",
        "ais_log.h": "c",
        "pthread.h": "c",
        "cstring": "c",
        "cstdlib": "c",
        "ios": "cpp"
